{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    },
    {
      "src": "./ml_engine/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/ml_engine/main.py"
    }
  ],
  "env": {
    "APP_MODULE": "main:app"
  }
}
